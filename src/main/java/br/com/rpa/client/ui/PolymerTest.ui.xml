<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p="urn:import:br.com.rpa.client.api.widgets.paper">
	<ui:style>
	.important {
		font-weight: bold;
	}
	.center {
		text-align: center;
	}
	</ui:style>
	<g:HTMLPanel styleName="{style.center}">
		<p:PaperButton ui:field="button" raised="true"></p:PaperButton>
	</g:HTMLPanel>
</ui:UiBinder> 